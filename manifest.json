{
  "name": "Linksplosion",
  "version": "0.4",
  "manifest_version": 2,
  "description": "Open all links in selected text.",
  "homepage_url": "https://github.com/ggreer/linksplosion",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {"scripts": ["background.js"]},
  "permissions": [
    "contextMenus",
    "<all_urls>"
  ]
}
